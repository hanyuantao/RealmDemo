GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/Realm" "${PODS_ROOT}/Headers/Realm/Realm"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/Realm/core"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/Realm" -isystem "${PODS_ROOT}/Headers/Realm/Realm"
OTHER_LDFLAGS = -ObjC -lc++ -lrealm-ios
PODS_ROOT = ${SRCROOT}/Pods